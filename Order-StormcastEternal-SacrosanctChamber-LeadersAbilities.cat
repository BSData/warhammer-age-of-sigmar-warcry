  <rules>
    <rule id="c00b-83eb-f437-acb7" name="⚁ [Double] Channelled Empowerment" hidden="true">
      <modifiers>
        <modifier type="set" field="hidden" value="false">
          <conditionGroups>
            <conditionGroup type="or">
              <conditions>
                <condition field="selections" scope="0ca4-e66d-9403-64a5" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="4702-646d-ff6b-28fe" type="atLeast"/>
                <condition field="selections" scope="f2ce-030d-9971-73f0" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="4702-646d-ff6b-28fe" type="atLeast"/>
                <condition field="selections" scope="5ed1-bb68-7a31-b173" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="4702-646d-ff6b-28fe" type="atLeast"/>
              </conditions>
            </conditionGroup>
          </conditionGroups>
        </modifier>
      </modifiers>
      <description>Until the end of this fighter&apos;s activation, add the value of this ability to the Strength characteristic of the next attack action made by this fighter that has a Ranger characteristic of 3 or less.</description>
    </rule>
    <rule id="0074-6f62-3ae8-0b69" name="⚁ [Double] Cleanse the Realm of Taint" hidden="true">
