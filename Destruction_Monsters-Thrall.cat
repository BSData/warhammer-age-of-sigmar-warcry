                    <condition field="selections" scope="ancestor" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="7c12-f3a7-3b3c-30f1" type="instanceOf"/>
                  </conditions>
                </conditionGroup>
              </conditionGroups>
