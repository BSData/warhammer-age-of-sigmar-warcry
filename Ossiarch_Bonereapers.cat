                <condition field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="4992-8710-6244-a9b8" type="atLeast"/>
              </conditions>
            </conditionGroup>
          </conditionGroups>
        </modifier>
      </modifiers>
      <description>Remove up to 1 damage point allocated to each friendly fighter within 6” of this fighter.</description>
    </rule>
    <rule id="b49f-269d-92dd-2ac9" name="⚃ [Quad] Soulreaper Scythe" hidden="true">
      <modifiers>
        <modifier type="set" field="hidden" value="false">
